{
	"name": "qa-studio",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"postinstall": "prisma generate || echo 'Prisma generate skipped'",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:e2e:headless": "playwright test --headed=false"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@fontsource/fira-mono": "^5.2.7",
		"@inlang/paraglide-js": "^2.3.2",
		"@neoconfetti/svelte": "^2.2.2",
		"@playwright/test": "^1.55.1",
		"@qastudio-dev/playwright": "^1.1.1",
		"@skeletonlabs/skeleton": "^4.1.4",
		"@skeletonlabs/skeleton-svelte": "^4.1.4",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^22.18.12",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"mdsvex": "^0.12.6",
		"playwright": "^1.55.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"dependencies": {
		"@neodrag/svelte": "^2.3.3",
		"@prisma/client": "^6.18.0",
		"@stripe/stripe-js": "^8.1.0",
		"@vercel/blob": "^2.0.0",
		"decap-cms-app": "^3.8.4",
		"dotenv": "^17.2.3",
		"gray-matter": "^4.0.3",
		"lucide-svelte": "^0.548.0",
		"marked": "^16.4.1",
		"nanoid": "^5.1.6",
		"openai": "^6.7.0",
		"prisma": "^6.18.0",
		"shiki": "^3.13.0",
		"stripe": "^19.1.0",
		"svelte-clerk": "^0.17.6"
	}
}
